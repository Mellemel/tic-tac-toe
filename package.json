{
  "name": "tic-tac-toe",
  "version": "0.0.0",
  "description": "A tic tac toe game",
  "scripts": {
    "prebuild": "npm run clean-build && npm run test",
    "build": "npm webpack --progress --colors && server:build",
    "dev": "npm-run-all --parallel test:watch server:dev",
    "server:build": "babel-node tools/server.js",
    "server:dev": "webpack-dev-server",
    "clean-build": "npm run remove-build && mkdir build",
    "remove-build": "rimraf ./build",
    "test": "mocha tools/testsetup.js /src/**.test.js --reporter progreess --require mocha-clean",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Mellemel/tic-tac-toe.git"
  },
  "author": "Melvin Canela <M.Canela92@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mellemel/tic-tac-toe/issues"
  },
  "homepage": "https://github.com/Mellemel/tic-tac-toe#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "browser-sync": "^2.15.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.0.2",
    "mocha-clean": "^1.0.0",
    "node-sass": "^3.9.3",
    "npm-run-all": "^3.1.0",
    "postcss-loader": "^0.13.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  }
}
